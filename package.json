{
    "name": "events-manager",
    "version": "1.0.0",
    "description": "Events Manager provides a platform that helps manage creation and development of events such as conferences, ceremonies, weddings, formal parties, concerts, or conventions and as well provide centers to host these events. Suppose a user has an event coming up, He/She can post it on Events Manager and get a center.",
    "main": "app.js",
    "engines": {
        "node": "8.8.1",
        "npm": "5.4.2"
    },
    "scripts": {
        "react": "./node_modules/.bin/webpack-dev-server --config ./webpack.config.js",
        "start:dev": "nodemon --exec babel-node -- ./server/bin/www.js",
        "test": "npm run reset:db && npm run migrate:test && npm run seed:test && nyc mocha --require babel-polyfill --compilers js:babel-register ./server/test/test.js",
        "build": "babel server -d dist",
        "start": "babel-node -- ./server/bin/www.js",
        "test:dev:t": "./node_modules/.bin/cross-env NODE_ENV=test sequelize db:migrate:undo:all && ./node_modules/.bin/cross-env NODE_ENV=test sequelize db:migrate && cross-env NODE_ENV=test nyc mocha --require babel-polyfill --compilers js:babel-register ./server/test/test.js --exit",
        "heroku-postbuild": "npm run build",
        "coverage": "nyc report --reporter=text-lcov | coveralls",
        "seed:test": "./node_modules/.bin/cross-env NODE_ENV=test sequelize db:seed:all",
        "migrate": "./node_modules/.bin/sequelize db:migrate",
        "migrate:test": "./node_modules/.bin/cross-env NODE_ENV=test sequelize db:migrate",
        "reset:db": "./node_modules/.bin/cross-env NODE_ENV=test node ./server/test/resetHelper.js",
        "test:dev:ed": "npm run setup:db && npm run setup:db2 && npm run test:dev",
        "setup:db": "./node_modules/.bin/cross-env NODE_ENV=test sequelize db:migrate:undo:all",
        "setup:db2": "./node_modules/.bin/cross-env NODE_ENV=test sequelize db:migrate",
        "test:dev": "./node_modules/.bin/cross-env NODE_ENV=test nyc mocha --require babel-polyfill --compilers js:babel-register ./server/test/test.js --exit"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Abudu-Samuel/Events-Manager.git"
    },
    "author": "Samuel Abudu",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/Abudu-Samuel/Events-Manager/issues"
    },
    "homepage": "https://github.com/Abudu-Samuel/Events-Manager#readme",
    "devDependencies": {
        "cross-env": "^5.1.1",
        "eslint": "^4.11.0",
        "eslint-config-airbnb-base": "^12.1.0",
        "eslint-plugin-import": "^2.8.0",
        "eslint-plugin-jsx-a11y": "^6.0.2",
        "eslint-plugin-react": "^7.5.1",
        "html-webpack-plugin": "^2.30.1",
        "mocha": "^4.0.1",
        "nodemon": "^1.12.1",
        "nyc": "^11.3.0",
        "webpack": "^3.8.1",
        "webpack-dev-server": "^2.9.4"
    },
    "dependencies": {
        "babel-cli": "^6.26.0",
        "babel-core": "^6.26.0",
        "babel-eslint": "^8.0.2",
        "babel-loader": "^7.1.2",
        "babel-plugin-transform-object-rest-spread": "^6.26.0",
        "babel-preset-es2015": "^6.24.1",
        "babel-preset-react": "^6.24.1",
        "babel-preset-stage-0": "^6.24.1",
        "babel-register": "^6.26.0",
        "bcrypt": "^1.0.3",
        "body-parser": "^1.18.2",
        "chai": "^4.1.2",
        "coveralls": "^3.0.0",
        "css-loader": "^0.28.7",
        "dotenv": "^4.0.0",
        "eslint-config-react": "^1.1.7",
        "express": "^4.16.2",
        "extract-text-webpack-plugin": "^3.0.2",
        "file-loader": "^1.1.5",
        "image-webpack-loader": "^3.4.2",
        "jsonwebtoken": "^8.1.0",
        "morgan": "^1.9.0",
        "node-sass": "^4.7.2",
        "pg": "^7.4.0",
        "pg-hstore": "^2.3.2",
        "react": "^16.1.1",
        "react-dom": "^16.1.1",
        "react-google-maps": "^9.4.1",
        "react-router": "^3.2.0",
        "sass-loader": "^6.0.6",
        "sequelize": "^4.22.8",
        "sequelize-cli": "^3.1.0",
        "style-loader": "^0.19.0",
        "supertest": "^3.0.0"
    }
}
