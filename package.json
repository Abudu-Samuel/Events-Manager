{
    "name": "events-manager",
    "version": "1.0.0",
    "description": "Events Manager provides a platform that helps manage creation and development of events such as conferences, ceremonies, weddings, formal parties, concerts, or conventions and as well provide centers to host these events. Suppose a user has an event coming up, He/She can post it on Events Manager and get a center.",
    "main": "app.js",
    "engines": {
        "node": "8.8.1",
        "npm": "5.4.2"
    },
    "scripts": {
        "react": "./node_modules/.bin/webpack-dev-server --config ./webpack.config.js",
        "start:dev": "nodemon --exec babel-node -- ./server/bin/www.js",
        "build": "npm-run-all --parallel build:client build:server",
        "build:client": "rimraf client-dist && webpack --config webpack.config.prod.js",
        "build:server": "rimraf server-dist && babel server -d server-dist",
        "start": "npm run build && node server-dist/bin/distServer.js",
        "test": "cross-env NODE_ENV=test sequelize db:migrate:undo:all && cross-env NODE_ENV=test sequelize db:migrate && cross-env NODE_ENV=test nyc mocha --compilers js:babel-register ./server/test/index.js --exit",
        "heroku-postbuild": "npm run build",
        "coverage": "nyc report --reporter=text-lcov | coveralls",
        "seed:test": "./node_modules/.bin/cross-env NODE_ENV=test sequelize db:seed:all",
        "migrate": "./node_modules/.bin/sequelize db:migrate",
        "migrate:test": "./node_modules/.bin/cross-env NODE_ENV=test sequelize db:migrate",
        "reset:db": "./node_modules/.bin/cross-env NODE_ENV=test node ./server/test/resetHelper.js",
        "test:dev:ed": "npm run setup:db && npm run setup:db2 && npm run test:dev",
        "setup:db": "./node_modules/.bin/cross-env NODE_ENV=test sequelize db:migrate:undo:all",
        "setup:db2": "./node_modules/.bin/cross-env NODE_ENV=test sequelize db:migrate",
        "test:dev": "./node_modules/.bin/cross-env NODE_ENV=test nyc mocha --require babel-polyfill --compilers js:babel-register ./server/test/test.js --exit"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Abudu-Samuel/Events-Manager.git"
    },
    "author": "Samuel Abudu",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/Abudu-Samuel/Events-Manager/issues"
    },
    "homepage": "https://github.com/Abudu-Samuel/Events-Manager#readme",
    "devDependencies": {
        "cross-env": "^5.1.1",
        "eslint": "^4.19.1",
        "eslint-config-airbnb-base": "^12.1.0",
        "eslint-plugin-import": "^2.10.0",
        "eslint-plugin-jsx-a11y": "^5.1.1",
        "eslint-plugin-react": "^7.5.1",
        "mocha": "^4.0.1",
        "nodemon": "^1.12.1",
        "nyc": "^11.3.0",
        "webpack-dev-server": "^2.9.4"
    },
    "dependencies": {
        "axios": "^0.17.1",
        "babel-cli": "^6.26.0",
        "babel-core": "^6.26.0",
        "babel-eslint": "^8.0.2",
        "babel-loader": "^7.1.2",
        "babel-plugin-transform-object-rest-spread": "^6.26.0",
        "babel-polyfill": "^6.26.0",
        "babel-preset-env": "^1.6.1",
        "babel-preset-es2015": "^6.24.1",
        "babel-preset-react": "^6.24.1",
        "babel-preset-stage-0": "^6.24.1",
        "babel-register": "^6.26.0",
        "bcrypt": "^1.0.3",
        "body-parser": "^1.18.2",
        "chai": "^4.1.2",
        "classnames": "^2.2.5",
        "coveralls": "^3.0.0",
        "css-loader": "^0.28.7",
        "dotenv": "^4.0.0",
        "dotenv-webpack": "^1.5.5",
        "eslint-config-react": "^1.1.7",
        "eslint-plugin-flowtype": "^2.46.1",
        "express": "^4.16.2",
        "extract-text-webpack-plugin": "^3.0.2",
        "file-loader": "^1.1.5",
        "history": "^4.7.2",
        "html-webpack-plugin": "^2.30.1",
        "image-webpack-loader": "^3.4.2",
        "jsonwebtoken": "^8.1.0",
        "lodash": "^4.17.5",
        "morgan": "^1.9.0",
        "node-sass": "^4.7.2",
        "npm-run-all": "^4.1.2",
        "pg": "^7.4.0",
        "pg-hstore": "^2.3.2",
        "prop-types": "^15.6.0",
        "react": "^16.1.1",
        "react-dom": "^16.1.1",
        "react-paginate": "^5.2.2",
        "react-redux": "^5.0.6",
        "react-router-dom": "^4.2.2",
        "react-router-redux": "^4.0.8",
        "redux": "^3.7.2",
        "redux-devtools-extension": "^2.13.2",
        "redux-logger": "^3.0.6",
        "redux-thunk": "^2.2.0",
        "rimraf": "^2.6.2",
        "sass-loader": "^6.0.6",
        "sequelize": "^4.22.8",
        "sequelize-cli": "^3.1.0",
        "style-loader": "^0.19.0",
        "supertest": "^3.0.0",
        "sweetalert": "^2.1.0",
        "thunk": "0.0.1",
        "webpack": "^3.8.1",
        "webpack-dev-middleware": "^1.12.2",
        "webpack-hot-middleware": "^2.21.0"
    }
}
